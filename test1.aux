function r=test1
r.pass=0
r.fail=0

a=[1:3 8:2:25]
lena1=a.length
a(3:5)=90:92
lena2=a.length

ch = a(1:3).sum==93 && lena1==lena2 && a(5:7).sum==[92 12 14].sum
r = check_condition(r, ch, "LHS indexing1 --- a(3:5)=90:92")


function p = check_condition(p, cond, msgtag)
if cond
	printf("OK\t%s\n", msgtag)
	p.pass +=1
else
	printf("FAIL\t%s\n", msgtag)
	p.fail +=1
end
	

	